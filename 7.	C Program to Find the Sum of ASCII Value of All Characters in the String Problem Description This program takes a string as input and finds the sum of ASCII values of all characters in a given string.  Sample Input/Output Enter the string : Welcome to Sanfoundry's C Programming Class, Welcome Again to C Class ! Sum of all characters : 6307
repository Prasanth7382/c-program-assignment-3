#include <stdio.h>
#include <string.h>

int main()
{
    char str[1000];
    int sum = 0;
    int len, i;

    printf("Enter the string : ");
    gets(str);

    len = strlen(str);

    for(i=0; i<len; i++)
    {
        sum += (int)str[i];
    }

    printf("Sum of all characters : %d", sum);

    return 0;
}
